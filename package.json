    "frontend:start": "nx run-many --target=serve --projects=frontend-main --parallel --maxParallel=20",
    "codegen": "graphql-codegen --config codegen.config.yml -w",
    "commit": "git-cz",
    "release": "standard-version -a"
  },
  "private": true,
